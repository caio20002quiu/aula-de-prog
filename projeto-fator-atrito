    float massasele;
    float viscosidadesele;

    float massaA = 998.2;
	float viscosidadeA= 1.99;

	float massaAR = 1.189;
	float viscosidadeAR = 1.83;

	float massaE = 789;
	float viscosidadeE = 1.19;

	float massaG = 1260;
	float viscosidadeG = 1.49;

	float massaM = 13550;
	float viscosidadeM = 1.56;

	float massaO = 891;
	float viscosidadeO = 0.29;



	while (erro1 == 1){

	cout<<"\n|---TABELA DE FLUIDOS---|:\n";


